<!DOCTYPE html>
<html>
<head>
  <title>Demo_App | <%= yield(:title) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag :defaults %> 
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
 <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
      <%= debug(params) if Rails.env.development? %>
      <%= render 'layouts/footer' %>
    </div>
 </body>
</html>
